{
"manifest_version": 2,
    "name": "Kaca Mata Bitcoin by @roaringstars",
    "description": "Seeing price in Satoshi",
    "version": "1.0",
    "homepage_url": "https://roaringstars.com",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}